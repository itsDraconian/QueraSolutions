#include <iostream>
using namespace std;
int main()
{
    int a,x=0;
    cin>>a;
    int a1=a;
    while(a > 0) 
	{
        x = (x * 10) + (a % 10);
        a /= 10;
    }
	int b,y=0;
    cin>>b;
    int b1=b;
    while(b > 0) 
	{
        y = (y * 10) + (b % 10);
        b /= 10;
    }
    if(x>y)
    	{
    		cout << b1 << " < " << a1;
		}
	else if(x<y)
		{
			cout << a1 << " < " << b1;
		}
	else
		{
			cout << b1 << " = " << a1;
		}
	return 0;
}

