#include <iostream>
using namespace std;
 
int main()
{
 int num[12];
 int sum;
 cin >> num[1] >> num[2] >> num[3] >> num[4] >> num[5] >> num[6] >> num[7] >> num[8] >> num[9] >> num[10] >> num[11] >> num[12];
 sum = num[1] + num[2]  + num[3] + num[4] + num[5] + num[6] + num[7] + num[8] + num[9] + num[10] + num[11] + num[12] ;
 cout << sum;
 return 0;
}
