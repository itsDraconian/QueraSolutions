#include <iostream>
using namespace std;
int main()
	{
	int r,y,g;
	int r1=0,r2=0,r3=0,r4=0,r5=0;
	int y1=0,y2=0,y3=0,y4=0,y5=0;
	int g1=0,g2=0,g3=0,g4=0,g5=0;
	char str[100];
	cin >> str;
	
	// T1
	if(str[0]=='R')
	    {
			r1=1;
	    }
	else if(str[0]=='Y')
	    {
	        y1=1;
	    }
	else
	    {
	       g1=1;
	    }
	
	// T2
	if(str[1]=='R')
	    {
			r2=1;
	    }
	else if(str[1]=='Y')
	    {
	        y2=1;
	    }
	else
	    {
	       g2=1;
	    }
	    
	// T3
	if(str[2]=='R')
	    {
			r3=1;
	    }
	else if(str[2]=='Y')
	    {
	        y3=1;
	    }
	else
	    {
	       g3=1;
	    }
	    
	// T4
	if(str[3]=='R')
	    {
			r4=1;
	    }
	else if(str[3]=='Y')
	    {
	        y4=1;
	    }
	else
	    {
	       g4=1;
	    }
	    
	// T5
	if(str[4]=='R')
	    {
			r5=1;
	    }
	else if(str[4]=='Y')
	    {
	        y5=1;
	    }
	else
	    {
	       g5=1;
	    }
	r=r1+r2+r3+r4+r5;
	y=y1+y2+y3+y4+y5;
	g=g1+g2+g3+g4+g5;
	
	if(r>=3)	
		{
			cout << "nakhor lite";
		}
	else if(r>=2 && y>=2)
		{
			cout << "nakhor lite";
		}
	else if(g==0)
		{
			cout << "nakhor lite";
		}
	else
		{
			cout << "rahat baash";
		}
	return 0;
	}
