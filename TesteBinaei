#include <iostream>
using namespace std;

int main()
{
int x;
cin >> x;
char numx[x];
for(int i=0;i<x;i++)
	{
		cin >> numx[i];
	}

char numy[x];
for(int j=0;j<x;j++)
	{
		cin >> numy[j];
	}
	
int t=0;
for(int k=0;k<x;k++)
	{
		if(numx[k] != numy[k])
			{
			 t++;
			}
	}
cout << t;
return 0;
}    
