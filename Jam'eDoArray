#include <iostream>
using namespace std;
 
int main()
{
 int n;
 cin >> n;
 int num1[n];
 int i;
 for(i=0;i<n;i++)
    {
        cin >> num1[i];
    }
 int num2[n];
 int j;
 for(j=0;j<n;j++)
    {
        cin >> num2[j];
    }
 int num3[n];
 int k;
 for(k=0;k<n;k++)
 	{
 	num3[k] = num1[k] + num2[k];
    }
 for(k=0;k<n;k++)
 	{
 		cout << num3[k] << " ";
	}
 return 0;
}
